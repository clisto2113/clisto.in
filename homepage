<script>
    // DOM Elements
    const installBtn = document.getElementById('installBtn');
    const pwaInstallBtn = document.getElementById('pwaInstallBtn');
    const installPopup = document.getElementById('installPopup');
    const closePopup = document.getElementById('closePopup');
    const iosInstallGuide = document.getElementById('iosInstallGuide');
    
    // Global variable to store the install prompt event
    let deferredPrompt;

    // Check if app is already installed
    function isInstalled() {
        return window.matchMedia('(display-mode: standalone)').matches || 
               window.navigator.standalone || 
               document.referrer.includes('android-app://');
    }

    // Check if user is on iOS
    function isIOS() {
        return /iPad|iPhone|iPod/.test(navigator.userAgent) || 
              (navigator.platform === 'MacIntel' && navigator.maxTouchPoints > 1);
    }

    // Set button to installed state
    function setAsInstalled() {
        installBtn.textContent = 'App Installed';
        installBtn.classList.add('installed');
        installBtn.disabled = true;
    }

    // Handle install button click
    function handleInstallClick() {
        if (isInstalled()) {
            setAsInstalled();
            return;
        }
        installPopup.classList.add('active');
    }

    // Handle PWA install button click
    async function handlePWAInstallClick() {
        if (deferredPrompt) {
            try {
                deferredPrompt.prompt();
                const { outcome } = await deferredPrompt.userChoice;
                
                if (outcome === 'accepted') {
                    setAsInstalled();
                    installPopup.classList.remove('active');
                    return;
                }
            } catch (error) {
                console.error('PWA installation failed:', error);
            }
        }
        
        // Fallback to direct download
        window.location.href = "https://www.clisto.in/Products/doc";
    }

    // Initialize the install button
    function initInstallButton() {
        if (isInstalled()) {
            setAsInstalled();
        }
        
        if (isIOS()) {
            iosInstallGuide.style.display = 'block';
        }
    }

    // Event Listeners
    installBtn.addEventListener('click', handleInstallClick);
    pwaInstallBtn.addEventListener('click', handlePWAInstallClick);
    closePopup.addEventListener('click', () => installPopup.classList.remove('active'));
    
    installPopup.addEventListener('click', (e) => {
        if (e.target === installPopup) {
            installPopup.classList.remove('active');
        }
    });
    
    // Capture the beforeinstallprompt event
    window.addEventListener('beforeinstallprompt', (e) => {
        e.preventDefault();
        deferredPrompt = e;
    });
    
    // Detect successful installation
    window.addEventListener('appinstalled', () => {
        setAsInstalled();
    });
    
    // Initialize when DOM is loaded
    document.addEventListener('DOMContentLoaded', initInstallButton);

    // Register Service Worker
    if ('serviceWorker' in navigator) {
        window.addEventListener('load', () => {
            navigator.serviceWorker.register('/sw.js')
                .then(registration => {
                    console.log('ServiceWorker registration successful');
                })
                .catch(error => {
                    console.log('ServiceWorker registration failed:', error);
                });
        });
    }
</script>



 <script>
    const manifest = {
        "name": "Clisto Stock Analysis",
        "short_name": "Clisto",
        "start_url": "/",
        "display": "standalone",
        "background_color": "#000000",
        "theme_color": "#4621ff",  // Changed from #8733f5 to #4621ff
        "description": "Stock market analysis powered by Clisto.",
        "icons": [
            {
                "src": "https://www.clisto.in/clisto_vui.png",
                "sizes": "192x192",
                "type": "image/png"
            },
            {
                "src": "https://www.clisto.in/clisto_vui.png",
                "sizes": "512x512",
                "type": "image/png"
            }
        ]
    };

    // Create and append the manifest
    const manifestBlob = new Blob([JSON.stringify(manifest)], { type: 'application/json' });
    const manifestURL = URL.createObjectURL(manifestBlob);
    const manifestLink = document.createElement('link');
    manifestLink.rel = 'manifest';
    manifestLink.href = manifestURL;
    document.head.appendChild(manifestLink);

    // Additional theme color meta tag for immediate browser UI changes
    const metaThemeColor = document.createElement('meta');
    metaThemeColor.name = 'theme-color';
    metaThemeColor.content = '#4621ff';
    document.head.appendChild(metaThemeColor);

    // For Safari/iOS specific theming
    const appleMeta = document.createElement('meta');
    appleMeta.name = 'apple-mobile-web-app-status-bar-style';
    appleMeta.content = '#4621ff';
    document.head.appendChild(appleMeta);
</script>




