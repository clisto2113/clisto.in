<script>
  // Wait for DOM to be ready
  document.addEventListener("DOMContentLoaded", function () {
    const loader = document.querySelector(".clisto-bubble-loader");
    let angle = 0;

    function rotateLoader() {
      angle = (angle + 1) % 360;
      loader.style.transform = `rotate(${angle}deg)`;
      requestAnimationFrame(rotateLoader);
    }

    rotateLoader();
  });
</script>

