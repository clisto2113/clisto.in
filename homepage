        <!-- Example Image -->
        <!-- Breadcrumb History (placed over the image) -->


<script>





        // Function to extract and display breadcrumbs
        function generateBreadcrumbs() {
            // Get the breadcrumb JSON-LD data from the script tag
            const jsonLdData = JSON.parse(document.querySelector('script[type="application/ld+json"]').textContent);

            // Get the breadcrumb list container
            const breadcrumbList = document.getElementById('breadcrumb-list');
            
            // Iterate through each breadcrumb item and create a list item
            jsonLdData.itemListElement.forEach(item => {
                const listItem = document.createElement('li');
                
                // Create a link for the breadcrumb
                const link = document.createElement('a');
                link.href = item.item;
                link.textContent = item.name;
                
                // Append the link to the list item
                listItem.appendChild(link);
                
                // Append the list item to the breadcrumb list
                breadcrumbList.appendChild(listItem);
            });
        }

        // Generate breadcrumbs on page load
        window.onload = generateBreadcrumbs;
    </script>



<script src="https://code.iconify.design/3/3.0.0/iconify.min.js"></script>

    <script>




        $(document).ready(function() {
    var username = localStorage.getItem('username');
    
document.getElementById('name').value= username;
 
    fetchProfileData(username);

    // Function to fetch profile data via AJAX
    function fetchProfileData(username) {
    
 
        $.ajax({
           url: 'Products/fa.php',
                    method: 'POST',
                    dataType: 'json',
                    data: { username: username },
            success: function(data) {
                displayProfileData(data);
            },
            error: function(xhr, status, error) {
                
            }
        });
    }

    // Function to display profile data on the webpage
    function displayProfileData(data) {
        // Check if data contains error
        if (data.error) {
            
            return;
        }

        // Escape HTML entities in data
       const escapedUsername = escapeHtml(data.username);
        const escapedTitle = escapeHtml(data.title);
        const escapedStatus = escapeHtml(data.status);
        const escapedProfile = data.profile;
localStorage.setItem("profileIm", escapedProfile);
   function checkTitleAndProfile(escapedTitle, escapedProfile) { 
    var profile = localStorage.getItem("profileIm");
     // Check if profile image is empty and show popup if true
    if (escapedProfile && escapedProfile.length <= 20) { 
      console.log(escapedProfile);
        $('#uniquePopupID').css('display', 'block'); 
        openSidebar(); // Open sidebar if profile image is not available
    } else {
        $('#uniquePopupID').css('display', 'none');
    }
    // Check if title is empty and show notification if true
    if (!escapedTitle) {
        $('#notification').css('display', 'block'); 
    } else {
        $('#notification').css('display', 'none');
    }









   
}

// Example usage after fetching or defining escapedTitle and escapedProfile
checkTitleAndProfile(escapedTitle, escapedProfile);

   
       

        // Display data in profileDataDiv
        var profileDataDiv = $('#profileData');
profileDataDiv.html(`    <a href="https://www.clisto.in/Dashboard/explore">
<div class="content-container" style="background:transparent;text-align: center;">

        <div class="profile-img-container">
            <img id="profileImage" src="data:image;base64,${escapedProfile}" alt="Profile Image" class="profile-img">
            <span class="status-dot ${escapedStatus === 'Online' ? 'online' : 'offline'}"></span> <!-- Status dot -->
        </div>
   

    <div class="post-details">
        <h2 class="username highlighted" style="color:white;">${escapedUsername}</h2>
        ${escapedTitle ? `<p class="post-title">${escapedTitle}</p>` : ''}
    </div>
</div></a>`);


}

// Function to escape HTML entities
function escapeHtml(unsafe) {
    return unsafe.replace(/[&<"']/g, function (m) {
        switch (m) {
            case '&':
                return '&amp;';
            case '<':
                return '&lt;';
            case '"':
                return '&quot;';
            case "'":
                return '&#39;';
            default:
                return m;
        }
    });
}


    
        });
    
</script>
 









