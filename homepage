<!-- Load GSAP CDN before the script -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js"></script>

  <script>
    const clisto = document.getElementById("clistoText");
    const slogan = document.getElementById("sloganText");

    function loopAnimation() {
      gsap.to(clisto, { duration: 0.6, opacity: 1, y: 0, ease: "power2.out" });
      gsap.to(slogan, { duration: 0.6, opacity: 0, y: 20, ease: "power2.in" });

      setTimeout(() => {
        gsap.to(clisto, { duration: 0.6, opacity: 0, y: -20, ease: "power2.in" });
        gsap.to(slogan, { duration: 0.6, opacity: 1, y: 0, ease: "power2.out" });
      }, 2000);

      setTimeout(() => {
        gsap.to(slogan, { duration: 0.6, opacity: 0, y: 20, ease: "power2.in" });
        gsap.to(clisto, { duration: 0.6, opacity: 1, y: 0, ease: "power2.out" });
      }, 4000);

      setTimeout(loopAnimation, 6000); // Repeat loop every 6s
    }

    loopAnimation();
  </script>

